    <magic priority="10">
      <match value="%" type="string" offset="0"/>
    </magic>
    <magic priority="50">
      <match value="documentclass" type="string" offset="1"/>
    </magic>
	<magic priority="100">
	  <match value="\\documentclass" type="string" offset="0:256" />
	</magic>
